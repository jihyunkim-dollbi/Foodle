<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sist.mapper.mypage-mapper">

<select id="mypageRerveList" resultType="ReservationVO" parameterType="string" >
	SELECT rno,userid,restotalprice,respeople,resdate,restime FROM reservation
	WHERE userid=#{userid}
</select>
<select id="mypageCouponList" resultType="CouponVO" parameterType="string">
	SELECT * FROM coupon
	WHERE userid=#{userid}
</select>


</mapper>